{
  "name": "ciot_supp",
  "version": "1.0.0",
  "description": "Route Master V3 UI",
  "author": "Douglas Blumeyer <dblumeyer@pivotal.io>",
  "private": true,
  "scripts": {
    "start": "node build/dev-server.js",
    "int": "node build/int-server.js",
    "start-prod": "node build/prod-server.js",
    "build": "node build/build.js",
    "e2e": "npm run kill-mock-big-schedules-server && ./verification/test/e2e/run_e2e_tests.sh",
    "e2e-win": "call ./verification/test/e2e/run-e2e-win.bat",
    "test": "npm run unit && npm run e2e-win",
    "unit": "npm run lint-fix && ava",
    "unit-ci": "ava ./verification/test/unit/specs/**/*.js !e2e/**",
    "lint": "eslint --ext .js,.vue src ./verification/test/e2e/specs ./verification/test/unit/specs",
    "lint-fix": "eslint --fix --ext .js,.vue src ./verification/test/e2e/specs ./verification/test/unit/specs",
    "update-licenses": "nlf > licenses.txt",
    "kill-test-server": "lsof -ti:8081 -sTCP:LISTEN | xargs kill",
    "kill-mock-big-schedules-server": "lsof -ti:3000 -sTCP:LISTEN | xargs kill",
    "kill-test-server-win": "call ./verification/test/e2e/kill-test-server.bat",
    "kill-mock-data-server-win": "call ./verification/test/e2e/kill-mock-data-server.bat",
    "start-test-server": "npm run kill-mock-data-server-win && concurrently \"call ./verification/test/e2e/start_test_server-win.bat\" \"call ./verification/test/e2e/start_mock_server-win.bat\"",
    "postinstall": "npm run-script build",
    "coverage": "nyc ava && nyc report --reporter=html"
  },
