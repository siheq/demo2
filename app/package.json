  "engines": {
    "node": "8.3.0",
    "npm": "5.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "ava": {
    "files": [
      "./verification/test/unit/specs/**/*.js",
      "!e2e/**"
    ],
    "powerAssert": false,
    "babel": "inherit",
    "require": [
      "./avaSetup.js"
    ]
  },
  "nyc": {
    "report-dir": "./verification/test/unit/report/coverage",
    "exclude": [
      "verification/**/*.js"
    ]
  },
